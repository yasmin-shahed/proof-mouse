Z
1. B -> ~X prem;
2. (Y \/ A) -> Z prem;
3. ~X -> Y prem;
4. Z \/ B prem;
5. B -> Y hs 1, 3;
6. ~Z -> ~(Y \/ A) cp 2;
7. ~~~Z -> ~(Y \/ A) dn 6;
8. ~~~~Z \/ ~(Y \/ A) imp 7;
9. ~~Z \/ ~(Y \/ A) dn 8;
10. Z \/ ~(Y \/ A) dn 9;
11. Z \/ (~Y /\ ~A)  dm_oa 10;
12. (Z \/ ~Y) /\ (Z \/ ~A) dist_oa 11;
13. Z \/ ~Y simpl 12;
14. ~Y \/ Z or_comm 13;
15. Y -> Z imp 14;
16. B -> Z hs 5, 15;
17. B \/ Z or_comm 4;
18. ~~B \/ Z dn 17;
19. ~B -> Z imp 18;
20. B \/ ~B prem;
21. Z de 20, 16, 19;
